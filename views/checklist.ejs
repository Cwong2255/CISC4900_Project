        <h1>Assignment Checklist</h1>

    <table>
        <thead>
            <tr>
                <th scope="col">Last Name</th>
                <th scope="col">First Name</th>
                <th scope="col">EmplID</th>
                <th scope="col">Orientation</th>
                <th scope="col">Intake Survey</th>
                <th scope="col">Form 1</th>
                <th scope="col">Attestation</th>
                <th scope="col">Waiver</th>
                <th scope="col">Project Proposal</th>
                <th scope="col">Form 2</th>
                <th scope="col">Interim Report</th>
                <th scope="col">Interim Supervisor Evaluation</th>
                <th scope="col">Final Report</th>
                <th scope="col">Final Presentation</th>
            </tr>
        </thead>
        <tbody>

            <% checkList.forEach(Check=>{  %>
                <tr>
                    <td><%= Check.lastName %></td>
                    <td><%= Check.firstName %></td>
                    <td><%= Check.ID %></td>
                    <td><%= Check.Orientation %></td>
                    <td><%= Check.Intake_Survey %></td>
                    <td><%= Check.Attestation %></td>
                    <td><%= Check.Form_1 %></td>
                    <td><%= Check.Waiver %></td>
                    <td><%= Check.Project_Proposal %></td>
                    <td><%= Check.Form_2 %></td>
                    <td><%= Check.Interim_Report %></td>
                    <td><%= Check.Interim_Supervisor_Evaluation %></td>
                    <td><%= Check.Final_Report %></td>
                    <td><%= Check.Final_Presentation %></td>
                </tr>
            <% }) %>
            
            
        </tbody>
    </table>
    </br></br>
    <h1> Announcement</h1>
    <table>
        <thead>
            <th scope="col">Date</th>
            <th scope="col">Announcement</th>
        </thead>
        <tbody>
            <% announceList.forEach(Announcement=>{  %>
                <tr>
                    <td><%= Announcement.PostDate %></td>
                    <td><%= Announcement.Announcement %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
       
